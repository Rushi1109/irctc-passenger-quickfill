{
  "manifest_version": 3,
  "name": "IRCTC Passenger AutoFill",
  "version": "1.0.0",
  "description": "Auto-fills passenger details on IRCTC with one click.",
  "permissions": [
    "storage", 
    "scripting", 
    "activeTab"
  ],
  "host_permissions": [
    "*://www.irctc.co.in/*"
  ],
  "action": {
    "default_popup": "popup.html"
  }
}
